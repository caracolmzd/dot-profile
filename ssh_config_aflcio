# Defaults
# NB: configs can only be set once, so defaults must come last.
Host *
	User mzd
	SendEnv LANG LC_*
	HashKnownHosts yes
	TCPKeepAlive yes
	ServerAliveInterval 20
	ServerAliveCountMax 6
	AddKeysToAgent yes
	ForwardAgent no
	IdentityFile ~/.ssh/id_rsa_aflcio
	# PubkeyAuthentication no
	# PasswordAuthentication yes
